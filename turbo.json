{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "@hello-pact-store/prisma#db-deploy": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@hello-pact-store/api#build": {
      "dependsOn": ["@hello-pact-store/prisma#db-deploy"]
    },
    "@hello-pact-store/api#dx": {
      "dependsOn": ["@hello-pact-store/prisma#dx"]
    },
    "@hello-pact-store/storefront#dx": {
      "dependsOn": ["@hello-pact-store/prisma#dx"]
    },
    "dx": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
