{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "@sat0shi-store/prisma#build": {
      "dependsOn": ["$DATABASE_URL"],
      "outputs": ["zod/**"]
    },
    "@sat0shi-store/prisma#deploy": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@sat0shi-store/api#build": {
      "dependsOn": ["@sat0shi-store/prisma#deploy"]
    },
    "@sat0shi-store/api#dx": {
      "dependsOn": ["@sat0shi-store/prisma#dx"]
    },
    "@sat0shi-store/storefront#dx": {
      "dependsOn": ["@sat0shi-store/prisma#dx"]
    },
    "dx": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": ["apps/storefront/.env", "packages/prisma/.env"]
}
