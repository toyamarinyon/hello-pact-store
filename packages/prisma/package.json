{
  "name": "@hello-pact-store/prisma",
  "version": "0.0.0",
  "private": true,
  "main": "index.ts",
  "types": "index.d.ts",
  "files": [
    "zod"
  ],
  "scripts": {
    "build": "yarn generate-schemas",
    "dx": "yarn db-setup",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "db-reset": "run-s db-nuke db-setup",
    "db-deploy": "yarn prisma migrate deploy",
    "db-studio": "yarn prisma studio",
    "db-setup": "run-s db-up db-deploy db-seed",
    "db-up": "docker-compose up -d",
    "db-seed": "yarn prisma db seed",
    "db-migrate": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "generate-schemas": "prisma generate && prisma format",
    "postinstall": "yarn generate-schemas"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.10.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0"
  },
  "prisma": {
    "seed": "ts-node ./seed.ts"
  }
}
